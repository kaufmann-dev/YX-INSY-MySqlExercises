                                            amount : {
                                                bsonType : "int"
                                            }
                                        }
                                     }
                                 },
                                 losses : {
                                     bsonType : "array",
                                     items : {
                                        bsonType : "object",
                                         required : ["type", "amount"],
                                         additionalProperties : false,
                                         properties : {
                                             type : {
                                                 enum : [
                                                    "HEAVY_INFANTRY", "LIGHT_INFANTRY", "CAVALRY", "MERCENARIES", "ARCHER", "CHARIOTS", "HEAVY_CAVALRY", "BEASTS", "WARSHIP", "SHIP", "ARQUEBUSES", "ARTILLERY", "HEAVY_ARTILLERY",
